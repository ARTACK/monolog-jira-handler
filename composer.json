{
    "name": "artack/monolog-jira-handler",
    "type": "library",
    "description": "jira handler for monolog",
    "keywords": ["monolog", "jira", "handler"],
    "license": "MIT",
    "authors": [
        {
            "name": "ARTACK WebLab GmbH",
            "homepage": "https://www.artack.ch"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "monolog/monolog": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^2.0",
        "php-http/message": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/client-common": "^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^2.0",
        "nyholm/psr7": "^1.1",
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {"Artack\\Monolog\\JiraHandler\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Artack\\Monolog\\JiraHandler\\": "tests/" }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    }
}
